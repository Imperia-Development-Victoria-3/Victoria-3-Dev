types imperia_vienna_congress 
{
    ### COUNTRIES TAB
	type imperia_vienna_congress_overview = flowcontainer {
        layoutpolicy_horizontal = expanding
        layoutpolicy_vertical = expanding
        #spacing = -50

        #size = { 100% 100% }
        #margin_left = 100
        align = top
        margin_top = 20
        direction = vertical
        flowcontainer = {
            spacing = 5
            margin_top = 20
            direction = vertical
            parentanchor = hcenter|top
            flowcontainer = { # Great Powers
                spacing = 5
                margin_top = 20
                direction = vertical
                parentanchor = hcenter
                textbox = {
                    autoresize = yes
                    text = "CONGRESS_GREAT_POWERS"
                    using = header_font_fancy
                    using = fontsize_mega
                }
                divider_decorative = {}
                
                textbox = {
                    visible = "[IsDataModelEmpty(GetGlobalList('imperia_congress_great_powers'))]"
                    autoresize = yes
                    text = "CONGRESS_PLEASE_RUN_THE_GAME_FOR_A_BIT"
                }
                fixedgridbox = {
                    addcolumn = 120
                    addrow = 95
                    datamodel_wrap = 4
                    flipdirection = yes
                    datamodel = "[GetGlobalList('imperia_congress_great_powers')]"
                    item = {
                        large_flag = {
                            using = tooltip_above
                            datacontext = "[Scope.AccessCountry]"						
                            blockoverride "flag_tooltip" {
                                tooltip = "[ContextualDiplomaticPact.GetTooltip]"
                            }
                            textbox = {
                                autoresize = yes
                                margin_top = 65
                                margin_left = 35
                                raw_text = "[Scope.AccessCountry.GetPrestige]"
                                align = center
                            }
                        }			
                    }
                }
            }
            divider_clean = {}
            
            flowcontainer = { # Major Powers
                spacing = 5
                direction = vertical
                textbox = {
                    autoresize = yes
                    text = "CONGRESS_MAJOR_POWERS"
                    using = header_font_fancy
                    using = fontsize_xxl
                    align = left
                }
                divider_clean = {}
                
                textbox = {
                    visible = "[IsDataModelEmpty(GetGlobalList('imperia_congress_major_powers'))]"
                    autoresize = yes
                    text = "CONGRESS_PLEASE_RUN_THE_GAME_FOR_A_BIT"
                }
                fixedgridbox = {
                    addcolumn = 80
                    addrow = 60
                    datamodel_wrap = 5
                    flipdirection = yes
                    datamodel = "[GetGlobalList('imperia_congress_major_powers')]"
                    item = {

                        small_flag = {
                            parentanchor = vcenter
                            using = tooltip_above
                            datacontext = "[Scope.AccessCountry]"
                            blockoverride "flag_tooltip" {
                                tooltip = "[ContextualDiplomaticPact.GetTooltip]"
                            }
                            textbox = {
                                autoresize = yes
                                margin_top = 30
                                margin_left = 15
                                raw_text = "[Scope.AccessCountry.GetPrestige]"
                                align = center
                            }
                        }
                    }
                }
            }
        }
    }
